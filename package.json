{
  "name": "adventofcode2022",
  "version": "1.0.0",
  "description": "My solutions for Advent of Code 2022",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "start:dev": "npx nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andlo779/adventOfCode2022.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andlo779/adventOfCode2022/issues"
  },
  "homepage": "https://github.com/andlo779/adventOfCode2022#readme",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.10",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(unit|spec)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
